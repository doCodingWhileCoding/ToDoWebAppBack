{
  "name": "todowebapp_back",
  "version": "1.0.0",
  "type": "module",
  "description": "the backend of ToDoWebApp",
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/index.js",
    "local": "NODE_ENV=local npm run start",
    "dev": "NODE_ENV=development npm run start",
    "docker-build-local": "docker compose -f docker-compose.local.yml build",
    "docker-up-local": "docker compose -f docker-compose.local.yml up",
    "docker-local": "npm run docker-build-local && npm run docker-up-local",
    "docker-build-dev": "docker build -f Dockerfile.dev -t node .",
    "docker-up-dev": "docker run -p 9000:9000 --name back --env-file .env node",
    "docker-dev": "npm run docker-build-dev && npm run docker-up-dev"
  },
  "author": "Nadir Boughlala Ruiz",
  "license": "ISC",
  "volta": {
    "node": "18.18.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.2",
    "mongoose-paginate-v2": "^1.7.4",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.7",
    "nodemailer-express-handlebars": "^6.1.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
